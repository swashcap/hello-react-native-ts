{
  "name": "HelloReactNativeTS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "find {src,storybook} -type f -name '*.js' -exec rm {} \\+",
    "format": "prettier --write '*.js' '{src,storybook}/**/*.{ts,tsx}'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:storybook": "STORYBOOK=1 react-native start --resetCache",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "reset": "watchman watch-del-all && find $(node -pe 'require(\"os\").tmpdir()') -maxdepth 1 -type d -name 'metro-cache' -exec rm -rf {} \\;",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-ondevice-actions": "^5.1.0-alpha.39",
    "@storybook/addon-ondevice-knobs": "^4.1.18",
    "@storybook/addon-ondevice-notes": "^4.1.18",
    "@storybook/react-native": "^4.1.18",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.18",
    "@types/react-native": "^0.57.59",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react-native": "^3.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier": "^1.17.1",
    "react-docgen-typescript": "^1.12.4",
    "react-dom": "^16.8.6",
    "react-native-typescript-transformer": "^1.2.12",
    "react-native-web": "^0.11.4",
    "react-styleguidist": "^9.1.2",
    "react-test-renderer": "16.8.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2"
  }
}
